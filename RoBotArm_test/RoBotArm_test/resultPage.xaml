<Window x:Class="RoBotArm_test.resultPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RoBotArm_test"
        mc:Ignorable="d"
        Title="resultPage" Height="450" Width="654">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="125*"/>
            <ColumnDefinition Width="27*"/>
        </Grid.ColumnDefinitions>
        <DataGrid x:Name="resultDatagrid" ItemsSource="{Binding}" Height="360" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <Button x:Name="button" Content="Export To Excel" HorizontalAlignment="Left" Margin="10,378,0,0" VerticalAlignment="Top" Width="93" Height="31" Click="Button_Click" Grid.Column="1"/>

    </Grid>
</Window>
